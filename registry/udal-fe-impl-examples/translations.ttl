@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix schema: <http://schema.org/>.
@prefix dcat: <http://www.w3.org/ns/dcat#>.
@prefix qb: <http://purl.org/linked-data/cube#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix udal: <http://purl.org/linked-data/FE/udal/#>.

@base <urn:fairease.eu:udal:example:translations>.

# Dataset demand register
<https://lab.fairease.eu/dataset-demand-register/registry.ttl>
    a schema:OfferCatalog ;
    schema:name "Dataset demand register"^^xsd:string ; 
    schema:description "Dataset demand register, containing turtle files for various datastructure definitions."^^xsd:string ; 
    schema:itemListElement <> ; 
.

<>
    a schema:Offer , qb:DataStructureDefinition , fno:Function ; 

    rdfs:label "Translations Offer Contract"@en;
    rdfs:comment "Describes the contract that returns translations of terms"@en;
    
    # result properties
    fno:returns ( udal:lang_prop udal:terms_prop ) ; 

    #expected structure of result properties
    # measure
    qb:component [ 
        a qb:ComponentSpecification ; 
        qb:measure udal:terms_prop ;
    ] ;
    
    # dimensions
    qb:component [ 
        a qb:ComponentSpecification ; 
        qb:dimension udal:lang_prop ;
    ] ;
.

## Dataset Properties ## 
udal:terms_prop
    a rdf:Property , qb:MeasureProperty, fno:Output ; 
    rdfs:label "terms"^^xsd:string ; 
    rdfs:seeAlso <http://www.w3.org/2004/02/skos/core#Concept> ; 
    rdfs:range xsd:integer ; 
.

udal:lang_prop 
    a rdf:Property , qb:DimensionProperty, fno:Output ; 
    rdfs:label "language"^^xsd:string ; 
    rdfs:seeAlso schema:language ; 
    rdfs:range xsd:string ; 
.